        "6c621860-c3dc-4526-8c94-ed0b10f07ca7": {
          "type": "text",
          "settings": {
            "title": "Let us do the shopping and chopping",
            "text": "Sit back and relax as our professional chefs prepare up to 90% of your meal. No more mincing or measuring!",
            "image": "shopify:\/\/shop_images\/asset__home--steps-22.jpg",
            "icon_image": "none"
          }
        },
        "b7527733-7ec5-4a81-a49f-b78da4c55db4": {
          "type": "text",
          "settings": {
            "title": "Receive your meals",
            "text": "You can choose convenient, contactless pick-up at one of our local kitchens. Or, we’ll deliver dinner kits right to your door.",
