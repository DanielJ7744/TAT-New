      "blocks": {
        "dc2a17ef-be18-4b5d-8605-86ec566aa18a": {
          "type": "text",
          "settings": {
            "title": "Pick your meals",
            "text": "By putting creative spins on classic flavors, we make dinnertime an experience. Choose chef-created meals à la carte from the best meal kit delivery service in the Carolinas.",
            "image": "shopify:\/\/shop_images\/asset__home--steps-11.jpg",
